{{#each model.Tables}}
CREATE TABLE {{Name}} (
{{#each ColumnValues}}
    {{Name}} {{dbtype TypeValue ~}}
    {{#if~ Length}}({{Length}}){{/if~}}
    {{#if~ Precision}}({{Precision}}, {{Scale}}){{/if~}}
    {{#if~ IsNullable}} NULL{{/if~}}
    {{#if~ HasDefaultValue}} DEFAULT {{DefaultValue}}{{/if~}}
    {{#unless~ @last}},{{/unless}}
{{/each}}
);
{{/each}}
